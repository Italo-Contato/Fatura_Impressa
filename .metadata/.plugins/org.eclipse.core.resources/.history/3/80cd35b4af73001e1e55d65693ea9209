package entidades;

public class Compra {
	private String nomeCompra;
	private double valor_compra;

	public Compra() {

	}

	public Compra(String nome_compra, double valor_compra) {
		this.nomeCompra = nome_compra;
		this.valor_compra = valor_compra;
	}

	public String getNomeCompra() {
		return nomeCompra;
	}

	public void setNomeCompra(String nome_compra) {
		this.nomeCompra = nome_compra;
	}

	public double getValorCompra() {
		return valor_compra;
	}

	public void setValorCompra(double valor_compra) {
		this.valor_compra = valor_compra;
	}

}
